{
    "name": "frosh/shopware-rector",
    "description": "Shopware specific Rector",
    "type": "rector-extension",
    "license": "mit",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "rector/rector": "~0.12"
    },
    "autoload": {
        "psr-4": {
            "Frosh\\Rector\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "update-with-dependencies": true,
        "sort-packages": true
    },
    "extra": {
        "rector": {
            "includes": [
                "config/config.php"
            ]
        }
    }
}
