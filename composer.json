{
    "name": "frosh/shopware-rector",
    "description": "Shopware specific Rector",
    "type": "rector-extension",
    "license": "mit",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "~0.12",
        "tracy/tracy": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Frosh\\Rector\\": "src"
        }
    },
    "autoload-dev": {
      "classmap": ["stubs"],
        "psr-4": {
            "Frosh\\Rector\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "**.php.inc"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "update-with-dependencies": true,
        "sort-packages": true
    },
    "extra": {
        "rector": {
            "includes": [
                "config/config.php"
            ]
        }
    }
}
